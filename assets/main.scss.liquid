/*============== Primary Menu============ */
.engo-nav-menu {
  li {
    position: relative;
  }
  ul.navbar-nav {
    margin: 0;
    padding: 0;
    list-style: none;
    > li { 
      border-bottom:1px solid #ebebeb;
      > a {              
        color: #999999;
        font-weight: bold;
        text-transform: uppercase !important;
        letter-spacing: 0.075em;
        display: block;          		
        position: relative;
        font-size: 13px;
        > .open-submenu {
          display:none;
          cursor: pointer;
          line-height: 12px;
          position: absolute;
          right: 0px;
          top: 50%;
          transform: translateY(-50%);
          -webkit-transform: translateY(-50%);
          -ms-transform: translateY(-50%);
          &:after {
            font-size: 15px;
            font-family: 'FontAwesome';
            content: "\f107";
            font-style: normal;
            font-weight: normal;
          }
        }
        > .open-submenu-desktop {
          display:block;
          cursor: pointer;
          line-height: 12px;
          position: absolute;
          right: 0px;
          top: 50%;
          transform: translateY(-50%);
          -webkit-transform: translateY(-50%);
          -ms-transform: translateY(-50%);
          &:after {
            font-size: 15px;
            font-family: 'FontAwesome';
            content: "\f107";
            font-style: normal;
            font-weight: normal;
          }
        }
        &:hover {
          color: #000000;
        }
      }
      ul.sub-menu-dropdown {
        padding: 10px 0px;
        left: 0px;
        min-width: 200px;
        top: 100%;
        background: #fff;
        z-index: 999;
        margin-top: 4px;
          border: 1px solid #ebebeb ;
      }
      ul.sub-menu-dropdown, ul.sub-menu-megamenu {
        margin: 0;
        padding: 0;
        list-style: none;
        .sub-text{
        line-height: 1.5;
        a {
          font-size: 14px;
          color: #222;
          line-height: 20px;
          text-transform: capitalize;
          &:hover {
            color: #ba933e;
          }
        }
        }
      }
      .searchform {
        margin: 10px;
        input[type="text"] {
          border-width: 1px;
        }
      }
      &.active-menu-item > a > .open-submenu:after, &:hover > a > .open-submenu:after {
        content: '\f106';
      }
      
    }
  }
  /* Responsive */
  @media ( max-width: 1024px ) {
    &{
      overflow-y: scroll;
      position: fixed;
      width: 100%;
      max-height: 100%;
      background-color: transparent;
      display: none;
      ul.navbar-nav {
        >li {
			border:none;
          > a {
            border-bottom: 1px solid #ebebeb;
            font-size: 12px !important;
            text-transform: uppercase !important;
            font-weight: bold;
            > .open-submenu {
              display:block;
              transform: translateY(-0%);
              -webkit-transform: translateY(-0%);
              -ms-transform: translateY(-0%); 
              height: 100%;
              line-height: 39px;
              right: 0;
              text-align: center;
              top: 0;
              width: 40px;
            }
            > .open-submenu-desktop {
              display:none;
              transform: translateY(-0%);
              -webkit-transform: translateY(-0%);
              -ms-transform: translateY(-0%); 
              height: 100%;
              line-height: 39px;
              right: 0;
              text-align: center;
              top: 0;
              width: 40px;
            }
            
          }
          &.active-menu-item{
            ul.sub-menu-dropdown {
              display:block !important;
            }
          }
          ul.sub-menu-dropdown {
            background-color: transparent !important;
            border:none;
            position: inherit;
            li {
              a {
                font-size: 14px !important;
                font-weight:bold;
                
              }
              &.awemenu-megamenu-item{
                .grid{
                width:200px !important;
                  .col{
                  width:100%;
                  }
                  .super{
                  
                  }
                }
              
              }
            }
          }
          
        }
      }

    }    
  }
}




/* =======collection page==============  */
#Collection{
  .widget-container{
        margin-bottom: 40px;
    border-bottom: 1px solid #ebebeb;
    padding-bottom: 5px;
    font-size: 14px;
    .widget-title{
      font-family:"lato";
        font-size: 18px;
      font-weight:bold;
        margin: 0 0 30px;
        text-transform: uppercase;
        letter-spacing: 0.075em;
    }
    ul{
        margin: 0;
        padding: 0;
        list-style: none;
      li{
          padding-bottom: 6px;
        a{
          	font-family:"lato";
            color: #666;
    		text-transform: capitalize;
          text-decoration:none;
          span.count{   
            padding-left: 17px;
            font-size: 12px;
            color: #cacaca;
          
          }
        }
      }
 
      .li-price{
        a{
          text-decoration:none;
         
          &.active{
            color: $main-color;
            display: block;
            position: relative;
            
            &:before{
          		content: "";
              display: block;
              width: 9px;
              height: 1px;
              background: $main-color;
              position: absolute;
              top: 13px;
              right: 5px;
              transform: rotateZ(45deg);
          }
            &:after{
              content: "";
              display: block;
              width: 1px;
              height: 9px;
              background: $main-color;
              position: absolute;
              top: 9px;
              right: 9px;
              transform: rotateZ(45deg);
          }
          }
        }
      }
  
    }
  }
  .products-sortby{
    padding:50px 0 30px 0;
    .result-count{
    	display:inline-block;
      &:after{
      content: '';
      width: 1px;
      height: 20px;
      display: inline-block;
      float: right;
    	background:#ebebeb;
      margin: 0 0px 0 15px ;
        
      }
    }
    .style-switch{
    	display:inline-block;
      a{
        color:#acacac;
        padding-right:10px;
        &.active{
        color:#000;
        }
      }
      &:after{
      content: '';
      width: 1px;
      height: 20px;
      display: inline-block;
      float: right;
    	background:#ebebeb;
      margin: 0 10px 0 5px ;
        
      }
    }
    .select-icon{
      	display:inline-block;
      select{
      border:none;
        &:focus{
        outline:none;
        }
      }
      &.right{
        float:right;
      }
  	}
  }
  .grid.grid--uniform{
  padding:30px 0 50px 0;
  }
  
  @media (max-width: 480px) {
    .products-sortby{
      .select-icon.right{
          display: block;
          float: left !important;
          margin-bottom: 30px;
      }
    }
}
}

/* ---------preloader--------------- */

.noo-spinner {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: {{settings.preloader_color}};
    {%if settings.preloader_enable_background_image %}
    {%capture _image %}preloader_image.jpg{%endcapture%}
    background-image: url({{_image|asset_url}});
    {%endif%}
    z-index: 999999;
    -webkit-transition: all .8s ease 0s;
    -o-transition: all .8s ease 0s;
    transition: all .8s ease 0s
}



.spinner {
    width: 150px;
    height: 150px;
    position: absolute;
    left: 43%;
    top: 50%;
    margin: 0 auto;
}
@media (max-width: 767px) {
  .spinner {
    width: 100px;
    height: 100px;
    position: absolute;
    left: 35%;
    top: 50%;
    margin: 0 auto;
}
}

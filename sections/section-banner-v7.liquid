
<div data-section-id="{{ section.id }}" data-section-type="section-banner-v6" style="{% if section.settings.margin_top != blank %}margin-top: {{section.settings.margin_top}}px;{%endif%} {% if section.settings.margin_bottom != blank %}margin-bottom: {{section.settings.margin_bottom}}px;{% endif %}">
  <section >
    <div class="container">
      <div class="list-adv10">
				<div class="row">
					<div class="col-md-8 col-sm-8 col-xs-12">
						<div class="item-banner10 banner-adv zoom-image line-scale">
							 <img  {%if section.settings.banner1_image == blank%} src="{{'ad10.jpg'| asset_url }}"{%else%} src="{{section.settings.banner1_image|img_url:'grande'}}"  {%endif%} alt="">
							<div class="banner-info style1 text-center text-uppercase">
								<h2 class="title30 color paci-font text-lowercase">{{section.settings.banner1_title}}</h2>
								<div class="type-of inline-block">
									<strong class="title90 poiret-font">{{section.settings.banner1_text}}</strong>
									<span class="title18">{{section.settings.banner1_text2}}</span>
								</div>
								<h2 class="title30 font-bold">{{section.settings.banner1_text3}}</h2>
								<a href="{{section.settings.banner1_url}}" class="white bg-color btn-arrow style2">{{section.settings.banner1_shopnow}}</a>
                              
							</div>
						</div>
					</div>
					<div class="col-md-4 col-sm-4 col-xs-12">
						<div class="item-banner10 banner-adv zoom-image overlay-image">
							<a href="{{section.settings.banner2_url}}" class="adv-thumb-link">
										
                                      <img  {%if section.settings.banner2_image == blank%} src="{{'ad20.jpg'| asset_url }}"{%else%} src="{{section.settings.banner2_image|img_url:'grande'}}"  {%endif%} alt="">
					
									</a>
							<div class="banner-info style2 text-uppercase">
								<h2 class="title30 font-bold"><span class="inline-block bg-white">{{section.settings.banner2_title}}</span></h2>
								<h3 class="inline-block title14 bg-white">
                                  
                                  <a href="{{section.settings.banner2_url}}" class="white bg-color btn-arrow style2">{{section.settings.banner2_shopnow}}</a>
                                 </h3>
							</div>
						</div>
						<div class="item-banner10 banner-adv zoom-image overlay-image">
                          <a href="{{section.settings.banner3_url}}" class="adv-thumb-link">
                            <img  {%if section.settings.banner3_image == blank%} src="{{'ad20.jpg'| asset_url }}"{%else%} src="{{section.settings.banner3_image|img_url:'grande'}}"  {%endif%} alt="">

                          </a>
                         
							<div class="banner-info style2 text-uppercase">
								<h2 class="title30 font-bold"><span class="inline-block bg-white">{{section.settings.banner3_title}}</span></h2>
								<h3 class="inline-block title14 bg-white">
                                  <a href="{{section.settings.banner3_url}}" class="white bg-color btn-arrow style2">{{section.settings.banner3_shopnow}}</a>
                             
                              
                              </h3>
							</div>
						</div>
					</div>
				</div>
			</div>
   

    </div>
  </section>
</div>


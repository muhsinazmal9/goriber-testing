<div class="col-md-{{row}} col-sm-4 col-xs-12">
<div class="section-post">
  <div class="section-post-media">
    <a href="/admin/products/new" title="{{ product.title }}">
        <img class="product-featured-image" src="{{'p1.png'| asset_url}}" alt="">

    </a>
  </div>
  <div class="section-post-body">
    <h4 class="section-post-title">
      <a href="/admin/products/new" title="EXAMPLE PRODUCT">EXAMPLE PRODUCT</a>
    </h4>

    <div class="clearfix"></div>
    <div class="section-post-price">
      {% capture price %}{{ product.price | money }}{% endcapture %}
          <span >$35.00</span>
          <del class="sale-price">$75.00</del>
    </div>
  </div>
</div>
</div>
